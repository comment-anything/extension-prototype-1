{ 
    "manifest_version": 2,
    "name": "Moz test 1",
    "version": "0.1",

    "description": "Shows fetch results in the browser",

    "icons": {
        "48": "icons/border-48.png",
        "98": "icons/luke2.svg"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["populate.js"]
        }
    ],

    "content_security_policy": {
        "extension_pages": "script-src 'self' https://example.com; object-src 'self'"
    },
      

    "permissions": [
        "cookies",
        "scripting",
        "webNavigation",
		"webRequest",
		"webRequestBlocking", 
		"<all_urls>"
    ]
}